    "keywords": [
        "subscription-webhooks",
        "laravel-subscription-webhooks",
        "laravel-appstore-server-notifications",
        "laravel-android-server-notifications",
        "laravel-appstore-server-webhooks",
        "laravel-android-server-webhooks",
        "laravel in app subscription webhooks",
        "laravel subscription notifications",
        "laravel subscription webhooks"
    ],
    "minimum-stability": "stable",
    "license": "MIT",
    "autoload": {
