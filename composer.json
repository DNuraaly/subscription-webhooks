            "./helpers.php"
        ]
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0",
        "bensampo/laravel-enum": "*",
        "ext-json": "*"
    },
    "authors": [
        {
            "name": "Daniiar Satybaldiev",
            "email": "satybaldiev@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Axel\\SubscriptionWebhooks\\NotificationServiceProvider"
            ]
        }
    }
